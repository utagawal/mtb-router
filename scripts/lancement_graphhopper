sudo docker run -p 8989:8989 bigmichi1/graphhopper --url https://download.geofabrik.de/europe/france/rhone-alpes-latest.osm.pbf --host 0.0.0.0


sudo wget https://github.com/graphhopper/graphhopper/releases/download/5.3/graphhopper-web-5.3.jar https://raw.githubusercontent.com/graphhopper/graphhopper/5.x/config-example.yml http://download.geofabrik.de/europe/france/rhone-alpes-latest.osm.pbf

wget /home/lpeller/graphhopper-web-5.3.jar /home/lpeller/config-example.yml /home/lpeller/rhone-alpes-latest.osm.pbf
java -Ddw.graphhopper.datareader.file=rhone-alpes-latest.osm.pbf -jar *.jar server config.yml
